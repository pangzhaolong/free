<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ResourceName">

    <!-- 中心背景 -->


    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="#FA8685" />
            <size
                android:width="10dp"
                android:height="10dp" />
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="#F5DBB0" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>
</layer-list>