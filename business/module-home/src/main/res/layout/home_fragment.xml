<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <com.scwang.smart.refresh.layout.SmartRefreshLayout
        android:id="@+id/home_srl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingClass">

        <com.scwang.smart.refresh.header.ClassicsHeader
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/home_title_bg_iv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/home_title_bg" />

            <ImageView
                android:id="@+id/home_title_circle_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/home_title_circle" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/home_search_layout"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="5dp">

                    <RelativeLayout
                        android:id="@+id/home_search_rl"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/home_search_bg">

                        <ImageView
                            android:id="@+id/home_search_icon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="10dp"
                            android:src="@drawable/home_jdd_search" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/home_search_bar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="10dp"
                            android:layout_toEndOf="@+id/home_search_icon"
                            android:background="@drawable/home_jdd_search_bg"
                            android:gravity="center_vertical"
                            android:hint="搜商品 领优惠券"
                            android:textSize="14sp"
                            tools:ignore="MissingConstraints" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="1dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginEnd="1dp"
                            android:layout_marginBottom="1dp"
                            android:background="@drawable/home_search_btn_bg"
                            android:gravity="center"
                            android:paddingStart="30dp"
                            android:paddingEnd="30dp"
                            android:text="搜索"
                            android:textColor="@color/white"
                            android:textSize="14sp" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/home_jdd_help"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignTop="@+id/home_search_rl"
                        android:layout_alignBottom="@+id/home_search_rl"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="10dp"
                        android:drawableTop="@drawable/home_jdd_help"
                        android:gravity="center"
                        android:paddingStart="10dp"
                        android:paddingTop="6dp"
                        android:paddingEnd="10dp"
                        android:text="教程"
                        android:textColor="#A9AEB1"
                        android:textSize="10sp"
                        android:visibility="gone"
                        tools:ignore="MissingConstraints,RtlSymmetry" />
                </RelativeLayout>

                <androidx.coordinatorlayout.widget.CoordinatorLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <com.google.android.material.appbar.AppBarLayout
                        android:id="@+id/home_app_bar_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/transparent"
                        android:orientation="vertical">

                        <com.google.android.material.appbar.CollapsingToolbarLayout
                            android:id="@+id/collapsing_toolbar"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:expandedTitleGravity="top"
                            app:expandedTitleMarginEnd="64dp"
                            app:expandedTitleMarginStart="48dp"
                            app:layout_scrollFlags="scroll|exitUntilCollapsed|snap">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:id="@+id/home_search_rl1"
                                    android:layout_width="match_parent"
                                    android:layout_height="45dp"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="10dp"
                                    android:background="@drawable/home_search_bg"
                                    android:visibility="gone">

                                    <ImageView
                                        android:id="@+id/home_search_icon1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="10dp"
                                        android:layout_marginEnd="10dp"
                                        android:src="@drawable/home_jdd_search" />

                                    <androidx.appcompat.widget.AppCompatTextView
                                        android:id="@+id/home_search_bar1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:layout_marginStart="5dp"
                                        android:layout_marginTop="5dp"
                                        android:layout_marginEnd="10dp"
                                        android:layout_toEndOf="@+id/home_search_icon1"
                                        android:background="@drawable/home_jdd_search_bg"
                                        android:gravity="center_vertical"
                                        android:hint="搜商品 领优惠券"
                                        android:textSize="14sp"
                                        tools:ignore="MissingConstraints" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:layout_alignParentEnd="true"
                                        android:layout_centerVertical="true"
                                        android:background="@drawable/home_search_btn_bg"
                                        android:gravity="center"
                                        android:paddingStart="20dp"
                                        android:paddingEnd="20dp"
                                        android:text="搜索"
                                        android:textColor="@color/white"
                                        android:textSize="14sp" />
                                </RelativeLayout>

                                <LinearLayout
                                    android:id="@+id/home_top_icon_ll"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginEnd="10dp"
                                    android:background="@drawable/home_banner_bg"
                                    android:gravity="center"
                                    android:orientation="horizontal"
                                    android:paddingTop="12dp"
                                    android:paddingBottom="15dp"
                                    android:weightSum="4">

                                    <LinearLayout
                                        android:id="@+id/home_title_tb"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/home_top_icon_iv1"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:src="@drawable/home_title_tb" />

                                        <TextView
                                            android:id="@+id/home_top_icon_tv1"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="8dp"
                                            android:gravity="center"
                                            android:text="淘宝福利"
                                            android:textColor="#2C2C2C"
                                            android:textSize="15sp"
                                            android:textStyle="bold" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/home_title_pdd"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/home_top_icon_iv2"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:src="@drawable/home_title_pdd" />

                                        <TextView
                                            android:id="@+id/home_top_icon_tv2"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="8dp"
                                            android:gravity="center"
                                            android:text="拼多多福利"
                                            android:textColor="#2C2C2C"
                                            android:textSize="15sp"
                                            android:textStyle="bold" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/home_title_jd"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/home_top_icon_iv3"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:src="@drawable/home_title_jd" />

                                        <TextView
                                            android:id="@+id/home_top_icon_tv3"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="8dp"
                                            android:gravity="center"
                                            android:text="京东福利"
                                            android:textColor="#2C2C2C"
                                            android:textSize="15sp"
                                            android:textStyle="bold" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:id="@+id/home_title_elm"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/home_top_icon_iv4"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:src="@drawable/home_title_elm" />

                                        <TextView
                                            android:id="@+id/home_top_icon_tv4"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_marginTop="8dp"
                                            android:gravity="center"
                                            android:text="外卖福利"
                                            android:textColor="#2C2C2C"
                                            android:textSize="15sp"
                                            android:textStyle="bold" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/home_banner_ll"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginTop="12dp"
                                    android:layout_marginEnd="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:background="@drawable/home_banner_bg"
                                    android:orientation="vertical">

                                    <RelativeLayout
                                        android:id="@+id/front_rp_rl"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        tools:ignore="MissingConstraints">

                                        <TextView
                                            android:id="@+id/front_rp_title"
                                            android:layout_width="wrap_content"
                                            android:layout_height="24dp"
                                            android:layout_marginStart="10dp"
                                            android:layout_marginTop="4dp"
                                            android:gravity="center"
                                            android:text="精选推荐"
                                            android:textColor="@color/black"
                                            android:textSize="17sp"
                                            android:textStyle="bold" />

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_alignParentEnd="true"
                                            android:layout_marginTop="4dp"
                                            android:gravity="center_vertical"
                                            android:orientation="horizontal">

                                            <FrameLayout
                                                android:id="@+id/home_random_users_fl"
                                                android:layout_width="wrap_content"
                                                android:layout_height="25dp"
                                                android:layout_gravity="center_vertical"
                                                android:layout_marginEnd="20dp"
                                                android:layout_toStartOf="@+id/front_cash_get_tv">

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_banner_header_riv1"
                                                    android:layout_width="25dp"
                                                    android:layout_height="25dp"
                                                    android:layout_gravity="center_vertical"
                                                    android:layout_marginStart="10dp"
                                                    app:type="circle"
                                                    tools:ignore="TooDeepLayout" />

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_banner_header_riv2"
                                                    android:layout_width="25dp"
                                                    android:layout_height="25dp"
                                                    android:layout_gravity="center_vertical"
                                                    android:layout_marginStart="30dp"
                                                    app:type="circle" />

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_banner_header_riv3"
                                                    android:layout_width="25dp"
                                                    android:layout_height="25dp"
                                                    android:layout_gravity="center_vertical"
                                                    android:layout_marginStart="50dp"
                                                    app:type="circle" />
                                            </FrameLayout>

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="28dp"
                                                android:background="@drawable/home_crazy_tip_bg"
                                                android:orientation="horizontal"
                                                android:paddingStart="8dp"
                                                android:paddingEnd="8dp">

                                                <ImageView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="center_vertical"
                                                    android:src="@drawable/home_crazy_tip_fire" />

                                                <TextView
                                                    android:id="@+id/front_cash_get_tv"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="match_parent"
                                                    android:gravity="center"
                                                    android:text=" 正在疯抢中 "
                                                    android:textColor="@color/white" />

                                                <ImageView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="center_vertical"
                                                    android:src="@drawable/home_banner_go" />
                                            </LinearLayout>
                                        </LinearLayout>

                                    </RelativeLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10dp"
                                        android:layout_marginBottom="14dp"
                                        android:orientation="horizontal"
                                        android:weightSum="4">

                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:orientation="vertical"
                                            android:paddingStart="10dp"
                                            android:paddingEnd="10dp">

                                            <FrameLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content">

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_seckil_riv1"
                                                    android:layout_width="62dp"
                                                    android:layout_height="62dp"
                                                    android:scaleType="fitXY"
                                                    app:corner_radius="5dp"
                                                    app:type="round"
                                                    tools:ignore="TooDeepLayout" />

                                                <TextView
                                                    android:id="@+id/home_seckil_tv1"
                                                    android:layout_width="62dp"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="left|bottom"
                                                    android:background="@drawable/home_banner_item_tip_bg"
                                                    android:gravity="center"
                                                    android:text="直降￥200"
                                                    android:textColor="@color/white"
                                                    android:textSize="12sp" />
                                            </FrameLayout>

                                            <LinearLayout
                                                android:layout_width="62dp"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginTop="10dp"
                                                android:gravity="center"
                                                android:orientation="horizontal">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:text="￥"
                                                    android:textColor="#E9423E"
                                                    android:textSize="10sp"
                                                    android:textStyle="bold" />

                                                <TextView
                                                    android:id="@+id/home_seckil_price_tv1"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:background="@drawable/home_banner_item_bg"
                                                    android:ellipsize="end"
                                                    android:gravity="bottom"
                                                    android:maxLines="1"
                                                    android:text="48"
                                                    android:textColor="#E9423E"
                                                    android:textSize="14sp"
                                                    android:textStyle="bold" />
                                            </LinearLayout>
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:orientation="vertical">

                                            <FrameLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content">

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_seckil_riv2"
                                                    android:layout_width="62dp"
                                                    android:layout_height="62dp"
                                                    android:scaleType="fitXY"
                                                    app:corner_radius="5dp"
                                                    app:type="round"
                                                    tools:ignore="TooDeepLayout" />

                                                <TextView
                                                    android:id="@+id/home_seckil_tv2"
                                                    android:layout_width="62dp"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="left|bottom"
                                                    android:background="@drawable/home_banner_item_tip_bg"
                                                    android:gravity="center"
                                                    android:text="直降￥200"
                                                    android:textColor="@color/white"
                                                    android:textSize="12sp" />
                                            </FrameLayout>

                                            <LinearLayout
                                                android:layout_width="62dp"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginTop="10dp"
                                                android:gravity="center"
                                                android:orientation="horizontal">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:text="￥"
                                                    android:textColor="#E9423E"
                                                    android:textSize="10sp"
                                                    android:textStyle="bold" />

                                                <TextView
                                                    android:id="@+id/home_seckil_price_tv2"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:background="@drawable/home_banner_item_bg"
                                                    android:ellipsize="end"
                                                    android:gravity="bottom"
                                                    android:maxLines="1"
                                                    android:text="48"
                                                    android:textColor="#E9423E"
                                                    android:textSize="14sp"
                                                    android:textStyle="bold" />
                                            </LinearLayout>
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:orientation="vertical">

                                            <FrameLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content">

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_seckil_riv3"
                                                    android:layout_width="62dp"
                                                    android:layout_height="62dp"
                                                    android:scaleType="fitXY"
                                                    app:corner_radius="5dp"
                                                    app:type="round"
                                                    tools:ignore="TooDeepLayout" />

                                                <TextView
                                                    android:id="@+id/home_seckil_tv3"
                                                    android:layout_width="62dp"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="left|bottom"
                                                    android:background="@drawable/home_banner_item_tip_bg"
                                                    android:gravity="center"
                                                    android:text="直降￥200"
                                                    android:textColor="@color/white"
                                                    android:textSize="12sp" />
                                            </FrameLayout>

                                            <LinearLayout
                                                android:layout_width="62dp"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginTop="10dp"
                                                android:gravity="center"
                                                android:orientation="horizontal">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:text="￥"
                                                    android:textColor="#E9423E"
                                                    android:textSize="10sp"
                                                    android:textStyle="bold" />

                                                <TextView
                                                    android:id="@+id/home_seckil_price_tv3"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:background="@drawable/home_banner_item_bg"
                                                    android:ellipsize="end"
                                                    android:gravity="bottom"
                                                    android:maxLines="1"
                                                    android:text="48"
                                                    android:textColor="#E9423E"
                                                    android:textSize="14sp"
                                                    android:textStyle="bold" />
                                            </LinearLayout>
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:orientation="vertical">

                                            <FrameLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content">

                                                <org.raphets.roundimageview.RoundImageView
                                                    android:id="@+id/home_seckil_riv4"
                                                    android:layout_width="62dp"
                                                    android:layout_height="62dp"
                                                    android:scaleType="fitXY"
                                                    app:corner_radius="5dp"
                                                    app:type="round"
                                                    tools:ignore="TooDeepLayout" />

                                                <TextView
                                                    android:id="@+id/home_seckil_tv4"
                                                    android:layout_width="62dp"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="left|bottom"
                                                    android:alpha="0.8"
                                                    android:background="@drawable/home_banner_item_tip_bg"
                                                    android:gravity="center"
                                                    android:text="直降￥200"
                                                    android:textColor="@color/white"
                                                    android:textSize="12sp" />
                                            </FrameLayout>

                                            <LinearLayout
                                                android:layout_width="62dp"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginTop="10dp"
                                                android:gravity="center"
                                                android:orientation="horizontal">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:text="￥"
                                                    android:textColor="#E9423E"
                                                    android:textSize="10sp"
                                                    android:textStyle="bold" />

                                                <TextView
                                                    android:id="@+id/home_seckil_price_tv4"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="bottom"
                                                    android:background="@drawable/home_banner_item_bg"
                                                    android:ellipsize="end"
                                                    android:gravity="bottom"
                                                    android:maxLines="1"
                                                    android:text="48"
                                                    android:textColor="#E9423E"
                                                    android:textSize="14sp"
                                                    android:textStyle="bold" />
                                            </LinearLayout>
                                        </LinearLayout>

                                    </LinearLayout>
                                </LinearLayout>

                            </LinearLayout>
                        </com.google.android.material.appbar.CollapsingToolbarLayout>

                        <com.google.android.material.tabs.TabLayout
                            android:id="@+id/home_category_tl"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:minWidth="10dp"
                            android:minHeight="35dp"
                            app:tabBackground="@color/transparent"
                            app:tabIndicatorColor="@color/transparent"
                            app:tabIndicatorHeight="0dp"
                            app:tabMaxWidth="200dp"
                            app:tabPaddingEnd="10dp"
                            app:tabPaddingStart="10dp"
                            app:tabRippleColor="@color/transparent"
                            app:tabSelectedTextColor="#F64A43"
                            app:tabTextColor="#6D6D6D"
                            tools:ignore="SpeakableTextPresentCheck" />
                    </com.google.android.material.appbar.AppBarLayout>

                    <androidx.viewpager2.widget.ViewPager2
                        android:id="@+id/home_category_vp2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#F5F5F5"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior" />
                </androidx.coordinatorlayout.widget.CoordinatorLayout>
            </LinearLayout>
        </FrameLayout>
    </com.scwang.smart.refresh.layout.SmartRefreshLayout>
</layout>
