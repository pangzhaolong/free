<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ResourceName">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="#A9AEB1" />
            <size
                android:width="10dp"
                android:height="10dp" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="#b9000000" />
            <size
                android:width="12dp"
                android:height="12dp" />
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval"
            android:useLevel="true">
            <solid android:color="#A9AEB1" />
        </shape>
    </item>
</layer-list>